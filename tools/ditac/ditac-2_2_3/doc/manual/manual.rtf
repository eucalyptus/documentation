{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
      <xsl:choose>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        <xsl:when test="@kind = 'attvalue'">
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
          <xsl:text>&quot;</xsl:text>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        </xsl:when>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        .
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        .
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        .
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        <xsl:when test="@kind = 'starttag'">
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
          <xsl:text>&lt;</xsl:text>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        </xsl:when>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
      </xsl:choose>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
      <xsl:apply-templates/>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
      <xsl:choose>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        <xsl:when test="@kind = 'attvalue'">
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
          <xsl:text>&quot;</xsl:text>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        </xsl:when>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        .
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        .
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
        .
