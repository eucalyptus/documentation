{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
  <xsl:import href="ditac-xsl:xhtml/xhtml.xsl"/>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
  <xsl:template match="*[contains(@class,' tech-d/tag ')]">
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
    <tt>
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
      <xsl:call-template name="commonAttributes"/>
