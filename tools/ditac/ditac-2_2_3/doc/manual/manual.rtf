}
{\plain\f0\fs20\cf2
\tab
}
}
{\plain\f0\fs20\cf2
Configure the 
}
{\plain\f2\fs18\cf2
Converter
}
{\plain\f0\fs20\cf2
.
}
\par
\pard\sb150\li465\ri65\sl216\cbpat21
{\plain\f2\fs18\cf2\chcbpat21
if (!converter.registerFOP(path("/opt/fop/fop"))) \{
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
    return 1;
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\}
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
File xslDir = new File(path("../../../xsl"));
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
URL xslDirURL = null;
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
try \{
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
    xslDirURL = xslDir.toURI().toURL();
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\} catch (MalformedURLException cannotHappen) \{\}
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
converter.setStyleSheetDirectory(xslDirURL);
}
\par
